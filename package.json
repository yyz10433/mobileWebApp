    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.13",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/test-utils": "^2.2.4",
    "@vue/tsconfig": "^0.1.3",
    "axios": "^1.2.0",
    "jsdom": "^20.0.3",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-px-to-viewport": "^1.1.1",
    "typescript": "~4.7.4",
    "unplugin-vue-components": "^0.22.11",
